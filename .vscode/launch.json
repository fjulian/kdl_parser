    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: run_manually.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/run_manually.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: run_auto.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/run_auto.py",
            "console": "integratedTerminal"
            // "args": ["--reuse-objects"]
        }
    ]
}
