    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: run.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/run.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: run_bt.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/run_bt.py",
            "console": "integratedTerminal",
            "args": ["--reuse-objects"]
        }
    ]
}
